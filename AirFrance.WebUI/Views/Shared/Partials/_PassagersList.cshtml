@using AirFrance.Model
@model Passager

<div class="row">
    <div class="col">@Model.Id</div>
    <div class="col">@Model.FirstName</div>
    <div class="col">@Model.LastName</div>
    <div class="col">@Model.Birthdate</div>
    <div class="col">@Model.IsCheckedId</div>
    <div class="col">@Model.TicketPrice</div>
    <div class="col">
        <p>@Model.Vol.Destination</p>
        <p>@Model.Vol.TakeOffDate</p>
    </div>

</div>
